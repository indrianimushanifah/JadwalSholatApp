<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.indriani.coding">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mekah"
        android:label="JadwalSholatApp"
        android:roundIcon="@mipmap/mekah"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="id.indriani.coding.produkok"></activity>
        <activity android:name="id.indriani.coding.DataDoa" />
        <activity android:name="id.indriani.coding.Doa_harian" />
        <activity android:name="id.indriani.coding.SholatApp" />
        <activity android:name="id.indriani.coding.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>